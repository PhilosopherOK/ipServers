CREATE TABLE Server(
    id   int PRIMARY KEY GENERATED BY DEFAULT AS IDENTITY,
    name varchar NOT NULL
);


CREATE TABLE IpS(
    id int PRIMARY KEY GENERATED BY DEFAULT AS IDENTITY,
    name varchar NOT NULL,
    created date,
    server_id int REFERENCES Server(id)
);

INSERT INTO server(name) VALUES ('USS');
